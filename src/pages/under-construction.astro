---
import Layout from '../layouts/Layout.astro';
import Goback from '../components/Goback';
---

<Layout>
  <section class="bg-white">
    <div class="container flex items-start min-h-screen px-6 mx-auto">
      <div
        class="flex flex-col items-center max-w-xl pt-20 mx-auto text-center"
      >
        <p
          class="p-3 text-sm font-medium text-blue-500 rounded-full bg-blue-50"
        >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 24 24"
            stroke-width="2"
            stroke="currentColor"
            class="w-10 h-10"
          >
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              d="M12 9v3.75m9-.75a9 9 0 11-18 0 9 9 0 0118 0zm-9 3.75h.008v.008H12v-.008z"
            ></path>
          </svg>
        </p>
        <h1
          class="mt-3 text-2xl lg:text-4xl font-semibold text-gray-800 md:text-3xl"
        >
          Page is Under Construction
        </h1>
        <p class="mt-4 mb-10 text-gray-500">
          We are working on this page. Please check back later.
        </p>
        <Goback text="Go Back" client:load />
      </div>
    </div>
  </section>
</Layout>
